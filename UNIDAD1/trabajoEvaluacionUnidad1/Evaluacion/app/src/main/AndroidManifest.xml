<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AlumnosApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AutenticacionYConsulta"
        tools:targetApi="31">
        <activity
            android:name=".ui.sicenet.Horario"
            android:exported="false"
            android:label="@string/title_activity_horario"
            android:theme="@style/Theme.AutenticacionYConsulta" />
        <activity
            android:name=".ui.sicenet.Finales"
            android:exported="false"
            android:label="@string/title_activity_finales"
            android:theme="@style/Theme.AutenticacionYConsulta" />
        <activity
            android:name=".ui.sicenet.Parciales"
            android:exported="false"
            android:label="@string/title_activity_parciales"
            android:theme="@style/Theme.AutenticacionYConsulta" />
        <activity
            android:name=".ui.sicenet.kardex"
            android:exported="false"
            android:label="@string/title_activity_kardex"
            android:theme="@style/Theme.AutenticacionYConsulta" />
        <activity
            android:name=".ui.sicenet.InfoStudent"
            android:exported="false"
            android:label="@string/title_activity_info_student"
            android:theme="@style/Theme.AutenticacionYConsulta" />
        <activity
            android:name=".ui.sicenet.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AutenticacionYConsulta">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>